      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/morajlab.bit/react-macos-ui",
            "tsConfig": "libs/morajlab.bit/react-macos-ui/tsconfig.lib.json",
            "project": "libs/morajlab.bit/react-macos-ui/package.json",
            "entryFile": "libs/morajlab.bit/react-macos-ui/index.ts",
            "external": ["react", "react-dom"],
            "babelConfig": "@nrwl/react/plugins/bundle-babel",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/morajlab.bit/react-macos-ui/**/*.{ts,tsx,js,jsx}"
