{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "official-website",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    },
    "@nrwl/gatsby": {
      "application": {
        "style": "css"
      }
    }
  },
  "projects": {
    "official-website": {
      "root": "apps/official-website",
      "sourceRoot": "apps/official-website",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/official-website",
            "outputPath": "dist/apps/official-website"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "official-website:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "official-website:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "official-website:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/official-website"],
          "options": {
            "jestConfig": "apps/official-website/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/official-website/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "official-website-e2e": {
      "root": "apps/official-website-e2e",
      "sourceRoot": "apps/official-website-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/official-website-e2e/cypress.json",
            "tsConfig": "apps/official-website-e2e/tsconfig.e2e.json",
            "devServerTarget": "official-website:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "official-website:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/official-website-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "official-website-cms": {
      "root": "apps/official-website-cms",
      "sourceRoot": "apps/official-website-cms",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/official-website-cms",
            "outputPath": "dist/apps/official-website-cms"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "official-website-cms:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "official-website-cms:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "official-website-cms:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/official-website-cms"],
          "options": {
            "jestConfig": "apps/official-website-cms/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/official-website-cms/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      }
    },
    "official-website-cms-e2e": {
      "root": "apps/official-website-cms-e2e",
      "sourceRoot": "apps/official-website-cms-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/official-website-cms-e2e/cypress.json",
            "tsConfig": "apps/official-website-cms-e2e/tsconfig.e2e.json",
            "devServerTarget": "official-website-cms:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "official-website-cms:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/official-website-cms-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "official-website-utilities": {
      "root": "libs/official-website/utilities",
      "sourceRoot": "libs/official-website/utilities/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/official-website/utilities/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/official-website/utilities"],
          "options": {
            "jestConfig": "libs/official-website/utilities/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "official-website-types": {
      "root": "libs/official-website/types",
      "sourceRoot": "libs/official-website/types/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/official-website/types/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/official-website/types"],
          "options": {
            "jestConfig": "libs/official-website/types/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "official-website-core": {
      "root": "libs/official-website/core",
      "sourceRoot": "libs/official-website/core/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/official-website/core/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/official-website/core"],
          "options": {
            "jestConfig": "libs/official-website/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "official-website-lib": {
      "root": "libs/official-website/lib",
      "sourceRoot": "libs/official-website/lib/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/official-website/lib/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/official-website/lib"],
          "options": {
            "jestConfig": "libs/official-website/lib/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "global-assets": {
      "root": "apps/global-assets",
      "sourceRoot": "apps/global-assets",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/global-assets",
            "outputPath": "dist/apps/global-assets"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "global-assets:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "global-assets:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "global-assets:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/global-assets"],
          "options": {
            "jestConfig": "apps/global-assets/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/global-assets/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "global-assets-e2e": {
      "root": "apps/global-assets-e2e",
      "sourceRoot": "apps/global-assets-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/global-assets-e2e/cypress.json",
            "tsConfig": "apps/global-assets-e2e/tsconfig.e2e.json",
            "devServerTarget": "global-assets:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "global-assets:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/global-assets-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  }
}
