          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/react-html5up-templates/hyperspace/react-html5up-hyperspace/src/environments/environment.ts",
                  "with": "apps/react-html5up-templates/hyperspace/react-html5up-hyperspace/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "react-html5up-templates-hyperspace-react-html5up-hyperspace:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "react-html5up-templates-hyperspace-react-html5up-hyperspace:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/react-html5up-templates/hyperspace/react-html5up-hyperspace/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/react-html5up-templates/hyperspace/react-html5up-hyperspace"
          ],
          "options": {
            "jestConfig": "apps/react-html5up-templates/hyperspace/react-html5up-hyperspace/jest.config.js",
            "passWithNoTests": true
          }
        }
