{
  "version": 1,
  "projects": {
    "rview-client": {
      "root": "apps/rview-client",
      "sourceRoot": "apps/rview-client/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/rview-client",
            "index": "apps/rview-client/src/index.html",
            "main": "apps/rview-client/src/main.ts",
            "polyfills": "apps/rview-client/src/polyfills.ts",
            "tsConfig": "apps/rview-client/tsconfig.app.json",
            "assets": [
              "apps/rview-client/src/favicon.ico",
              "apps/rview-client/src/assets"
            ],
            "styles": ["apps/rview-client/src/styles.css"],
            "scripts": [],
            "webpackConfig": "libs/nx-polyfills/src/lib/webpack.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/rview-client/src/environments/environment.ts",
                  "with": "apps/rview-client/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "rview-client:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "rview-client:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/rview-client/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/rview-client/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "rview-client-e2e": {
      "root": "apps/rview-client-e2e",
      "sourceRoot": "apps/rview-client-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/rview-client-e2e/cypress.json",
            "tsConfig": "apps/rview-client-e2e/tsconfig.e2e.json",
            "devServerTarget": "rview-client:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "rview-client:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/rview-client-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "rview-app": {
      "root": "apps/rview-app",
      "sourceRoot": "apps/rview-app/src",
      "projectType": "application",
      "prefix": "rview-app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "nx-electron:build",
          "options": {
            "outputPath": "dist/apps/rview-app",
            "main": "apps/rview-app/src/main.ts",
            "tsConfig": "apps/rview-app/tsconfig.app.json",
            "assets": ["apps/rview-app/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/rview-app/src/environments/environment.ts",
                  "with": "apps/rview-app/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "nx-electron:execute",
          "options": {
            "buildTarget": "rview-app:build"
          }
        },
        "package": {
          "builder": "nx-electron:package",
          "options": {
            "name": "rview-app",
            "frontendProject": "rview-client",
            "outputPath": "dist/packages",
            "prepackageOnly": true
          }
        },
        "make": {
          "builder": "nx-electron:make",
          "options": {
            "name": "rview-app",
            "frontendProject": "rview-client",
            "outputPath": "dist/executables"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/rview-app/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/rview-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "react-hyper": {
      "root": "libs/react-hyper",
      "sourceRoot": "libs/react-hyper/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/react-hyper/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "build": {
          "builder": "@nrwl/web:package",
          "options": {
            "outputPath": "dist/libs/react-hyper",
            "tsConfig": "libs/react-hyper/tsconfig.lib.json",
            "project": "libs/react-hyper/package.json",
            "entryFile": "libs/react-hyper/src/index.ts",
            "external": ["react", "react-dom"],
            "babelConfig": "@nrwl/react/plugins/bundle-babel",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/react-hyper/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nx-polyfills": {
      "root": "libs/nx-polyfills",
      "sourceRoot": "libs/nx-polyfills/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/nx-polyfills/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/nx-polyfills/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/nx-polyfills",
            "tsConfig": "libs/nx-polyfills/tsconfig.lib.json",
            "packageJson": "libs/nx-polyfills/package.json",
            "main": "libs/nx-polyfills/src/index.ts",
            "assets": ["libs/nx-polyfills/*.md"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  }
}
