{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    },
    "@nrwl/gatsby": {
      "application": {
        "style": "css"
      }
    }
  },
  "projects": {
    "react-mdb-fluent": {
      "root": "libs/react-mdb-fluent",
      "sourceRoot": "libs/react-mdb-fluent/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/react-mdb-fluent",
            "tsConfig": "libs/react-mdb-fluent/tsconfig.lib.json",
            "project": "libs/react-mdb-fluent/package.json",
            "entryFile": "libs/react-mdb-fluent/src/index.ts",
            "external": ["react", "react-dom"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/react-mdb-fluent/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/react-mdb-fluent/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/react-mdb-fluent"],
          "options": {
            "jestConfig": "libs/react-mdb-fluent/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/react-mdb-fluent/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/react-mdb-fluent",
            "config": {
              "configFolder": "libs/react-mdb-fluent/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "react-mdb-fluent-e2e": {
      "root": "apps/react-mdb-fluent-e2e",
      "sourceRoot": "apps/react-mdb-fluent-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/react-mdb-fluent-e2e/cypress.json",
            "tsConfig": "apps/react-mdb-fluent-e2e/tsconfig.e2e.json",
            "devServerTarget": "react-mdb-fluent:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "react-mdb-fluent:storybook:ci"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/react-mdb-fluent-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  }
}
